
<launch>
  <param name="/use_sim_time" value="true" />

  <!-- start world -->
  <!-- <node name="gazebo" pkg="gazebo" type="gazebo" args="$(find your_pkg)/worlds/your.world" respawn="false" output="screen" /> -->
  <node name="gazebo" pkg="gazebo_ros" type="gazebo" respawn="false" output="screen" />

  <!-- start gui -->
  <node name="gazebo_gui" pkg="gazebo_ros" type="gui" respawn="false" output="screen"/>

  <!-- include our robots -->
  <include file="$(find multi_rover)/launch/multi_rover.launch"/>
</launch>
